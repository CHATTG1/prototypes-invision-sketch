{
  "name": "prototypes-invision-sketch",
  "description": "Translate your prototyping links back and forth between Sketch and Invision",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Prototypes Invision ↔︎ Sketch",
    "manifest": "src/manifest.json",
    "main": "prototypes-invision-sketch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "mathieudutour <mathieu@dutour.me>",
  "repository" : {
    "type": "git",
    "url": "git+https://github.com/mathieudutour/prototypes-invision-sketch.git"
  }
}
